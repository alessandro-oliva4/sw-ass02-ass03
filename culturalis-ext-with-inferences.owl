                                         rdfs:label "Ricopre il ruolo di"@it ,
                                                    "hasRole"@en ;
                                         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  http://culturalis.org/cult/0.1#include
<http://culturalis.org/cult/0.1#include> rdf:type owl:ObjectProperty ;
